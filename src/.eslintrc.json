{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "prettier",
    "next/core-web-vitals"
  ],
  "plugins": ["react", "prettier"],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "rules": {
    "prettier/prettier": "error",
    "react/prop-types": "on",
    "react/react-in-jsx-scope": "off"
  }
}
